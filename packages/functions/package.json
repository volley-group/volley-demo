{
  "name": "@braid/functions",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "sideEffects": false,
  "exports": {
    "./*": [
      "./src/*/index.ts",
      "./src/*.ts"
    ]
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "db": "sst shell -- drizzle-kit",
    "db:push": "sst shell drizzle-kit push",
    "db:studio": "sst shell drizzle-kit studio",
    "db:connect": "sst shell ./pgconnect.sh",
    "db:reset": "docker compose down && docker volume rm braid_pgdata && docker compose up"
  },
  "devDependencies": {
    "@octokit/webhooks-types": "^7.6.1",
    "@types/pg": "^8.11.10",
    "drizzle-kit": "^0.28.1"
  },
  "peerDependencies": {
    "@tsconfig/bun": "^1.0.7",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@clerk/backend": "^1.15.1",
    "@hono/clerk-auth": "^2.0.0",
    "@hono/trpc-server": "^0.3.4",
    "@hono/zod-validator": "^0.4.1",
    "@slack/web-api": "^7.8.0",
    "drizzle-orm": "^0.36.4",
    "drizzle-zod": "^0.5.1",
    "hono": "^4.6.3",
    "octokit": "^4.0.2",
    "pg": "^8.13.1",
    "rss-parser": "^3.13.0",
    "ulid": "^2.3.0",
    "valibot": "^0.42.1"
  }
}
